{
  "name": "@vibe-x-ai/leslie-cli",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "leslie": "bin/run.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --no-error-on-unmatched-pattern",
    "test": "pnpm --filter @vibe-x-ai/leslie-core build && pnpm build && vitest run",
    "test:e2e": "pnpm --filter @vibe-x-ai/leslie-core build && pnpm build && vitest run test/e2e"
  },
  "dependencies": {
    "@vibe-x-ai/leslie-core": "workspace:*",
    "minimist": "^1.2.8",
    "yaml": "^2.8.1"
  }
}
